<div class="container">
    {{#if staff.Owner}}
    <h2>Owner <span class="badge" style="vertical-align: super"> {{ staff.Owner.length }} </span></h2>
    <div class="row">
        {{#each staff.Owner}}
        <div class="col-md-2 col-sm-3 col-xs-6">
            <a href="/u/{{ player }}">
                <div class="text-center">
                    <img src="https://visage.surgeplay.com/bust/{{uuid}}" width="96" height="96">
                </div>
                <div class="text-center">
                    <b>{{player}}</b>
                </div>
            </a>
        </div>
        {{/each}}
    </div>

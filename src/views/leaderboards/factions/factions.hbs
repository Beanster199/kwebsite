    <div class="container">
        <div class="row">
            <div class="col-md-4">
                <div class="panel panel-default">
                    <div class="panel-heading text-center">
                        <strong>Play Time</strong>
                    </div>
                    <table class="table practice-leaderboard">
                        <tbody>
                            {{#each factions.playtime}}
                            <tr>
                                <td>{{ count }}.</td>
                                <td>
                                    <a href="/u/{{name}}">
                                        <img src="https://visage.surgeplay.com/face/32/{{ uuid }}" width="16"
                                            height="16">
                                        {{ name }}
                                    </a>
                                </td>
                                <td>{{#playtime playtime }}{{/playtime}}</td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="col-md-4">
                <div class="panel panel-default">
                    <div class="panel-heading text-center">
                        <strong>Kills</strong>
                    </div>
                    <table class="table practice-leaderboard">
                        {{#each factions.kills}}
                        <tr>
                            <td>{{ count }}.</td>
                            <td>
                                <a href="/u/{{ name }}">
                                    <img src="https://visage.surgeplay.com/face/32/{{ uuid }}" width="16" height="16">
                                    {{ name }}
                                </a>
                            </td>
                            <td>{{ kills }} Kills</td>
                        </tr>
                        {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="col-md-4">
                <div class="panel panel-default">
                    <div class="panel-heading text-center">
                        <strong>Diamonds Mined</strong>
                    </div>
                    <table class="table practice-leaderboard">

                        <tbody>
                            {{#each factions.diamonds}}
                            <tr>
                                <td> {{ count }}.</td>
                                <td>
                                    <a href="/u/{{name}}>">
                                        <img src="https://visage.surgeplay.com/face/32/{{ uuid }}" width="16"
                                            height="16">
                                        {{ name }}
                                    </a>
                                </td>
                                <td> {{ DIAMOND }}</td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="col-md-4">
                <div class="panel panel-default">
                    <div class="panel-heading text-center">
                        <strong>Deaths</strong>
                    </div>
                    <table class="table practice-leaderboard">

                        <tbody>
                            {{#each factions.deaths}}
                            <tr>
                                <td> {{ count }}.</td>
                                <td>
                                    <a href="/u/{{name}}>">
                                        <img src="https://visage.surgeplay.com/face/32/{{ uuid }}" width="16"
                                            height="16">
                                        {{ name }}
                                    </a>
                                </td>
                                <td> {{ deaths }} Deaths</td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="col-md-4">
                <div class="panel panel-default">
                    <div class="panel-heading text-center">
                        <strong>Exp Collected</strong>
                    </div>
                    <table class="table practice-leaderboard">

                        <tbody>
                            {{#each factions.expcollected}}
                            <tr>
                                <td> {{ count }}.</td>
                                <td>
                                    <a href="/u/{{name}}>">
                                        <img src="https://visage.surgeplay.com/face/32/{{ uuid }}" width="16"
                                            height="16">
                                        {{ name }}
                                    </a>
                                </td>
                                <td> {{ expcollected }}</td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

    </div>
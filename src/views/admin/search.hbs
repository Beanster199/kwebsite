<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">Players starting with "{{query}}" ({{length users}} results)</h3>
    </div>
    <div class="panel-body">
        <div class="row">
            {{#each users}}
            <div class="col-2 col-sm-2 col-md-2 col-lg-2">
                <a href="/admin/player/{{name}}">{{name}}</a>
            </div>
            {{/each}}
        </div>
    </div>
</div>